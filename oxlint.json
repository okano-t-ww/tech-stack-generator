{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "React": "readonly",
    "JSX": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "eqeqeq": "error",
    "no-var": "error",
    "prefer-const": "warn",
    "prefer-arrow-callback": "warn",
    "no-duplicate-imports": "error"
  },
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "pedantic": "off"
  }
}
